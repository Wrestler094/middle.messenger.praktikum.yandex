{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Mikhail Kovalev",
  "description": "Учебный проект курса «Мидл фронтенд-разработчик» от Яндекс Практикума",
  "keywords": [
    "webpack",
    "docker",
    "websocket",
    "typescript",
    "jest"
  ],
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "serve": "webpack serve",
    "start": "npm run build && node ./server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "eslint ./src/ --ext .ts",
    "lint:css": "stylelint \"**/*.css\"",
    "fix": "npm run fix:ts && npm run fix:css",
    "fix:ts": "eslint --fix ./src/ --ext .ts",
    "fix:css": "stylelint --fix \"**/*.css\"",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run test"
  },
  "dependencies": {
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "css-loader": "6.7.3",
    "eslint": "8.36.0",
    "eslint-config-standard-with-typescript": "27.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-webpack-plugin": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "mini-css-extract-plugin": "2.7.3",
    "prettier": "2.8.4",
    "style-loader": "3.3.2",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "ts-jest": "27.1.5",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "targets": {
    "default": {
      "sourceMap": false,
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "engines": {
    "node": ">=14.0"
  }
}
